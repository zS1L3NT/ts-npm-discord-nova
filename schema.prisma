datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Alias {
    guild_id String
    alias    String
    command  String

    @@id([alias, command])
}

model Entry {
    guild_id       String  @id
    prefix         String?
    log_channel_id String?
}
